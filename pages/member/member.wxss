.mcontainer{
  height: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0;
  box-sizing: border-box;
  width: 100%;
  font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
  position: absolute;
  z-index:0;

  margin: 0px;
  margin-left: 0;
  margin-top: 0px;
  padding-bottom: 70px;
  opacity: 0;
}

.footer{
  display:flex;
  justify-content: space-between;
  bottom: 0pt;
	margin: 0pt;
  position: fixed;
  margin-bottom:0px;
  width: 100%;
  height: 50px;
  border-top: 1px solid #fff;
  background: #334054;
}

